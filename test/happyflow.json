  {
    "test": "field with dot notation",
    "hrq": "birth.date is greater than 1990-01-01",
    "mongoq": {
      "birth.date": {
        "$gt": {
          "$date": "1990-01-01T00:00:00.000Z"
        }
      }
    },
    "result": [
      "Aphrodite",
      "Athena",
      "BB King",
      "Demeter",
      "Hera",
      "Little John",
      "Maria"
    ]
  },
  {
    "test": "true keyword",
    "hrq": "\"is worth dying for\" is true",
